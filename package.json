{
  "name": "maffix-web",
  "version": "2.0.0",
  "description": "Independent musician fan engagement platform with gamified experiences - Maffix Edition",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "turbo run db:studio",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}\""
  },
  "keywords": [
    "musician",
    "fan-engagement",
    "gamification",
    "nextjs",
    "typescript",
    "supabase",
    "turborepo",
    "maffix"
  ],
  "author": {
    "name": "Maffix Team",
    "email": "team@maffix.com",
    "url": "https://maffix.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaonancui/maffix-web.git"
  },
  "bugs": {
    "url": "https://github.com/xiaonancui/maffix-web/issues"
  },
  "homepage": "https://maffix.com",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0"
  },
  "packageManager": "cnpm@9.0.0"
}
